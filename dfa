START(no error):
    /: COMMENT1
    ': QUOTE
    ": QUOTE
    \: SKIP
    EOF: END
    other: START

COMMENT1(no error): 
    *: COMMENT2
    ': QUOTE
    ": QUOTE
    \: SKIP
    EOF: END
    other: START

COMMENT2(no error):
    *: ENDCOMMENT1
    EOF: ERROR
    other: COMMENT2

ENDCOMMENT1
    /: START
    EOF: ERROR
    other COMMENT2

QUOTE(no error):
    ': START
    ": START
    \: SKIP
    other: QUOTE
    EOF: END

SKIP(no error):
    EOF: END
    other: QUOTE

ERROR(error):
    all: ERROR

END(no error):
    all: END

